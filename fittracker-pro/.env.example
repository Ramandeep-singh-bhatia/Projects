# FitTracker Pro - Environment Configuration
# Copy this file to .env and update with your values

# Database Configuration
POSTGRES_USER=fittracker
POSTGRES_PASSWORD=changeme_strong_password_here
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=changeme_redis_password_here

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=kafka:9092

# Eureka Service Discovery
EUREKA_SERVER_URL=http://eureka-server:8761/eureka/

# JWT Security
# Generate a secure 256-bit secret key
# You can use: openssl rand -base64 32
JWT_SECRET=changeme_your_256_bit_secret_key_here
JWT_EXPIRATION=86400000

# Service Ports
EUREKA_PORT=8761
CONFIG_SERVER_PORT=8888
API_GATEWAY_PORT=8080
USER_SERVICE_PORT=8081
NUTRITION_SERVICE_PORT=8082
WORKOUT_SERVICE_PORT=8083
ANALYTICS_SERVICE_PORT=8084

# Monitoring Ports
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
JAEGER_PORT=16686

# Grafana Configuration
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=changeme_grafana_password_here

# Spring Profiles
SPRING_PROFILES_ACTIVE=prod
